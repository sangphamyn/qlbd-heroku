<div class="container">
    <div class="mt-2">
        <div class="row">

            <table class="table table-bordered table-sm w-75 m-auto">
                <thead>
                    <tr class="bg-success text-white">
                    <th scope="col">Date</th>
                    <th scope="col">Time</th>
                    <th scope="col">Team A</th>
                    <th scope="col">Team B</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each match}}
                        <tr>
                            <td class="text-center">{{this.datepr}}</td>
                            <td class="text-center">{{this.timepr}}</td>
                            <td class="text-center">{{this.team_A}}</td>
                            <td class="text-center">{{this.team_B}}</td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>

        </div>
    </div>
</div>