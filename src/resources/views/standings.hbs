<div class="container">
    <div class="my-4">
        <table class="table table-striped table-sm w-75 table-bordered m-auto">
        <thead>
            <tr>
            <th scope="col">Rank</th>
            <th scope="col">Club</th>
            <th scope="col">MP</th>
            <th scope="col">W</th>
            <th scope="col">D</th>
            <th scope="col">L</th>
            <th scope="col">GF</th>
            <th scope="col">GA</th>
            <th scope="col">GD</th>
            <th scope="col">Pts</th>
            </tr>
        </thead>
        <tbody>
            {{#each standings}}
            <tr>
            <th class="text-center" scope="row">{{this.index}}</th>
            <td>{{this.team}}</td>
            <td class="text-center">{{this.mp}}</td>
            <td class="text-center">{{this.won}}</td>
            <td class="text-center">{{this.drawn}}</td>
            <td class="text-center">{{this.lost}}</td>
            <td class="text-center">{{this.gf}}</td>
            <td class="text-center">{{this.ga}}</td>
            <td class="text-center">{{this.gd}}</td>
            <td class="text-center">{{this.points}}</td>
            </tr>
            {{/each}}
        </tbody>

        </table>
    </div>
</div>